  # rdfs:isDefinedBy :brk;
# .

brk:Perceelnummerverschuiving a owl:Class;
  rdfs:label "Perceelnummerverschuiving"@nl;
  rdfs:isDefinedBy :brk;
  rdfs:comment "Toename van de X- en Y-coördinaat van het perceelnummer op de kaart om het perceelnummer te verschuiven naar een locatie waar meer ruimte is om het nummer af te beelden."@nl;
.

brk:KadastraleGrens a owl:Class;
  rdfs:label "KadastraleGrens"@nl;
  rdfs:isDefinedBy :brk;
  dct:subject brk_beg:KadastraleGrens;
.

brk:Aantekening a owl:Class;
  rdfs:label "Aantekening"@nl;
  rdfs:comment "Een aantekening betreft een verwijzing naar een stuk. Een aantekening geeft aan welk feit
er bestaat, genoemd in een stuk, dat gevolgen kan hebben voor de uitoefening van rechten op een onroerende
zaak."
  rdfs:isDefinedBy :brk;
.

brk:Stuk a owl:Class;
  rdfs:subClassOf foaf:Document,
    dbpo:Document,
    bibo:Document;
  rdfs:label "Stuk"@nl;
  rdfs:comment "Een Kadastraal stuk is een brondocument dat aanleiding geeft tot een wijziging van de gegevens in de BRK"@nl;
  rdfs:isDefinedBy :brk;
.

brk:Stukdeel a owl:Class;
  rdfs:subClassOf bibo:DocumentPart,
    doco:Section;
  rdfs:label "Stukdeel"@nl;
  rdfs:comment "Een Stukdeel is een (zelfstandig te onderkennen) deel van een stuk, op grond waarvan gegevens in de kadastrale registratie ontstaan, wijzigen, vervallen of worden aangevuld. Als in een stuk bijvoorbeeld is vermeld dat er verschillende rechten betrokken zijn bij verschillende leveringen (tegen verschillende koopsommen) dan worden die fragmenten uit de tekst van die akte als verschillende stukdelen onderkend. Dergelijke stukdelen kunnen in de toekomst afzonderlijk worden aangevuld, doorgehaald, en dergelijke. Een stukdeel komt vaak overeen met een rechtsfeit."@nl;
  rdfs:isDefinedBy :brk;
.

brk:Kadasterstuk a owl:Class;
  rdfs:subClassOf brk:Stuk;
  rdfs:label "Kadasterstuk"@nl;
  rdfs:comment "Een kadasterstuk is een stuk dat gevolgen kan hebben voor kadastrale en topografische
registratie, maar dat niet ter inschrijving in de Openbare registers is aangeboden"@nl;
  rdfs:isDefinedBy :brk;
.

brk:TerInschrijvingAangebodenStuk a owl:Class;
  rdfs:subClassOf brk:Stuk;
  rdfs:label "Ter inschrijving aangeboden stuk"@nl;
  rdfs:comment "Een stuk dat ter inschrijving in de openbare registers is aangeboden."@nl;
  rdfs:isDefinedBy :brk;
.

brk:Kadasterverzoek a owl:Class;
  rdfs:label "Kadasterverzoek"@nl;
  rdfs:comment "Een kadasterverzoek is een verzoek van het Kadaster aan de aanbieder van een ter inschrijving aangeboden stuk in verband met de afhandeling van het stuk."@nl;
  rdfs:isDefinedBy :brk;
.

brk:RedenVerzoek a owl:Class;
  rdfs:label "Reden verzoek"@nl;
  rdfs:comment "De reden voor het versturen van een kadasterverzoek naar de aanbieder van van een ter inschrijving aangeboden stuk."@nl;
  rdfs:isDefinedBy :brk;
.

brk:Ondertekenaar a owl:Class;
  rdfs:subClassOf foaf:Person;
  rdfs:label "Ondertekenaar"@nl;
  rdfs:comment "Een persoon die een aangeboden stuk heeft ondertekend.";@nl;
  rdfs:isDefinedBy :brk;
.

brk:Acgidentificatie a owl:Class;
  rdfs:label "ACG-identificatie"@nl;
  rdfs:comment "Identificatie van het Ambtelijk Correctiegegeven (ACG). Als het Kadaster constateert dat bijvoorbeeld een akte verkeerd in de kadastrale registratie is verwerkt, wordt dit met een ACG hersteld"@nl;
  rdfs:isDefinedBy :brk;
.

brk:Persoon a owl:Class;
  rdfs:label "Persoon"@nl;
  rdfs:comment "Iets dat rechten en plichten kan hebben."@nl;
  rdfs:isDefinedBy :brk;
.

brk:NatuurlijkPersoon a owl:Class;
  rdfs:subClassOf foaf:Person;
  rdfs:label "Natuurlijk persoon"@nl;
  rdfs:comment "Een mens, eventueel overleden."@nl;
  rdfs:isDefinedBy :brk;
  rdfs:subClassOf brk:Persoon;
.

brk:NietnatuurlijkPersoon a owl:Class;
  rdfs:label "Niet-natuurlijk persoon"@nl;
  rdfs:comment "Een niet-natuurlijk persoon is een rechtspersoon of samenwerkingsverband (bedrijf of organisatie)."@nl;
  rdfs:isDefinedBy :brk;
  rdfs:subClassOf brk:Persoon;
.

  
  
  
